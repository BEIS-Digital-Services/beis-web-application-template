{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplyEntityFrameworkMigrationsAtStartup": true,
  "CookieNamesConfiguration": {
    "essential": {
      "CookieAcceptedCookie": "CookieAccepted",
      "RememberSettingsCookie": "RememberSettingsConsent",
      "JSEnabledCookie": "js-enabled"
    },
    "nonEssential": {
      "AnalyticsCookie": "AnalyticsConsent",
      "MarketingCookie": "MarketingConsent"
    }
  }
}
